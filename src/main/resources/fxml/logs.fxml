<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="rootNode" spacing="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.omathe.restui.controller.LogsController">
   <children>
<!--       <HBox fillHeight="false" maxHeight="25.0" minHeight="25.0" prefHeight="25.0" VBox.vgrow="NEVER">  -->
      <HBox fillHeight="false" VBox.vgrow="NEVER">
         <children>
            <Button mnemonicParsing="false" onAction="#clear" text="clear" HBox.hgrow="NEVER" />
         </children>
      </HBox>
<!--       <HBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">  -->
      <HBox VBox.vgrow="ALWAYS">
         <children>
<!--             <TextArea id="logsArea" fx:id="logsArea" editable="false" maxHeight="1.7976931348623157E308" wrapText="true" HBox.hgrow="ALWAYS" /> -->
            <TextArea id="logsArea" fx:id="logsArea" editable="false" wrapText="true" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
