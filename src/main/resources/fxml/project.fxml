<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox onMouseExited="#mouseExited" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="restui.controller.ProjectController">
	<children>
		<TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tabClosingPolicy="UNAVAILABLE" HBox.hgrow="ALWAYS">
			<tabs>
				<Tab text="Project">
					<content>
						<AnchorPane>
							<children>
								<VBox prefHeight="600.0" prefWidth="607.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="5.0">
									<children>
                              <Label text="Base URL">
                                 <VBox.margin>
                                    <Insets left="5.0" right="5.0" top="10.0" />
                                 </VBox.margin></Label>
                              <TableView fx:id="baseUrlTable" editable="true" prefHeight="155.0" prefWidth="607.0">
                                <columns>
                                    <TableColumn fx:id="baseUrlEnabledColumn" prefWidth="75.0" text="Enabled" />
                                  <TableColumn fx:id="baseUrlNameColumn" prefWidth="145.0" text="Name" />
                                  <TableColumn fx:id="baseUrlUrlColumn" prefWidth="300.0" text="Url" />
                                </columns>
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="2.0" />
                                 </VBox.margin>
                              </TableView>
												<HBox fillHeight="false" spacing="5.0" VBox.vgrow="NEVER">
													<children>
														<Label text="Number of endpoints :" />
														<Label fx:id="nbEndpoints" text="Label" />
													</children>
													<padding>
														<Insets bottom="5.0" right="5.0" top="5.0" />
													</padding>
												</HBox>
									</children>
                           <padding>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </padding>
								</VBox>
							</children>
						</AnchorPane>
					</content>
				</Tab>
            <Tab text="Settings">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
										<VBox spacing="5.0">
											<children>
												<HBox fillHeight="false" spacing="5.0" VBox.vgrow="NEVER">
													<children>
														<ComboBox fx:id="parameterLocation" minWidth="-Infinity" prefWidth="110.0">
															<items>
																<FXCollections fx:factory="observableArrayList">
																	<String fx:value="HEADER" />
																	<String fx:value="QUERY" />
																</FXCollections>
															</items>
															<value>
																<String fx:value="HEADER" />
															</value>
														</ComboBox>
														<TextField fx:id="parameterName" promptText="Name" text="Authorization" />
														<TextField fx:id="parameterValue" promptText="Value" text="Bearer ">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                    </TextField>
														<Button minWidth="-Infinity" mnemonicParsing="false" onAction="#updateParametersValue" text="set" />
													</children>
													<VBox.margin>
														<Insets bottom="5.0" right="5.0" top="5.0" />
													</VBox.margin>
													<padding>
														<Insets bottom="5.0" right="5.0" top="5.0" />
													</padding>
												</HBox>
											</children>
										</VBox>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
			</tabs>
		</TabPane>
	</children>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>
	<effect>
		<Glow />
	</effect>
</HBox>
