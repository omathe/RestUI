<?xml version="1.0" encoding="UTF-8"?>
<project name="KOALA">
	<baseUrls>
		<baseUrl name="Local" url="http://192.168.5.11:8080/gms"
			enabled="true" />
		<baseUrl name="https" url="https://lorawan.italks.eu/oss"
			enabled="false" />
	</baseUrls>
	<path name="application">
		<path name="customers">
			<path name="{customerId}">
				<endpoint name="getCustomer"
					path="/application/customers/{customerId}" method="GET">
					<parameters>
						<parameter enabled="true" direction="REQUEST" location="HEADER" type="TEXT" name="Authorization" value="Bearer 123" />
						<parameter enabled="true" direction="REQUEST" location="QUERY" type="TEXT" name="fields" value="name" />
					</parameters>
				</endpoint>
			</path>
		</path>
	</path>
</project>
